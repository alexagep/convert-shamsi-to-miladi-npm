{
    "source": {
        "include": [
            "lib/convertors/gregorian.convertor/.",
            "lib/convertors"
        ],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs)"
    },
    "plugins": [
        "plugins/markdown",
        "jsdoc-route-plugin"
    ],
    "templates": {
        "cleverLinks": true,
		"monospaceLinks": true,
		"default": {
			"outputSourceFiles" : true
		},
        "applicationName": "Algorithmic Trading",
        "disqus": "",
        "googleAnalytics": "",
        "openGraph": {
            "title": "",
            "type": "website",
            "image": "",
            "site_name": "",
            "url": ""
        },
        "meta": {
            "title": "",
            "description": "",
            "keyword": ""
        },
        "linenums": true
    },
    "opts": {
        "encoding": "utf8",
        "destination": "docs/jsdoc/",
        "private": false,
        "lenient": true,
        "recurse": true,
        "verbose": true,
        "template": "node_modules/better-docs"
    }
}